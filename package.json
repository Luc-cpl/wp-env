{
  "name": "@luc-cpl/wp-setup",
  "version": "1.0.6",
  "description": "The WordPress environment setup tool.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Luc-cpl/wp-setup"
  },
  "keywords": [
    "cli",
    "wordpress",
    "setup",
    "environment",
    "docker",
    "wordpress-cli",
    "wp-cli",
    "development"
  ],
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:fresh": "rm -rf ./dist && tsc && tsc-alias",
    "prepublishOnly": "pnpm run build:fresh",
    "watch": "nodemon"
  },
  "bin": {
    "wp-setup": "dist/index.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Lucas Carvalho",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "edge.js": "^6.0.2",
    "unzipper": "^0.10.14"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/unzipper": "^0.10.9",
    "nodemon": "^3.1.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.5"
  }
}
